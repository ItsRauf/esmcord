  "license": "GPL-3.0",
  "devDependencies": {
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.45",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-glob": "^3.2.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.2.0"
  },
  "type": "module"
}
