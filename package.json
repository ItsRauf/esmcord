{
  "name": "esmcord",
  "version": "0.0.1-devel",
  "description": "swc build test",
  "main": "index.js",
  "scripts": {
    "precompile": "pnpm run lint",
    "compile": "swc src -d build",
    "postcompile": "node scripts/d.ts",
    "lint": "prettier --check \"src/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\"",
    "start": "node --unhandled-rejections=strict --trace-warnings --trace-uncaught build/mod.js"
  },
