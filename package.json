{
  "name": "esmcord",
  "version": "0.3.5",
  "description": "Discord library written in Typescript leveraging the latest ECMAScript features.",
  "keywords": [
    "discord",
    "typescript",
    "ecmascript",
    "discord.js"
  ],
  "main": "./build/mod.js",
  "files": [
    "build/*",
    "docs/*",
    ".eslintrc",
    ".prettierrc",
    ".swcrc",
    ".gitignore",
    "tsconfig.json"
  ],
  "scripts": {
    "precompile": "yarn run lint",
    "compile": "tsc -b",
    "docs": "typedoc --out docs src",
    "lint": "prettier --check \"src/**/*.ts\"",
    "postlint": "eslint src --ext .ts",
    "lint:fix": "prettier --write \"src/**/*.ts\"",
    "postlint:fix": "eslint src --ext .ts --fix"
  },
  "author": "Rauf",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsRauf/esmcord.git"
  },
  "types": "./build/mod.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "centra": "^2.5.0",
    "discord-api-types": "^0.23.1",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.93",
    "@types/centra": "^2.2.0",
    "@types/node": "^16.10.2",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "typedoc": "^0.22.5",
    "typescript": "^4.4.3"
  }
}
